---
import { teams } from "../data/team.json";
import TeamList from "../components/TeamList.astro";

---
<section class="team" id="team" data-aos="fade-up" data-aos-duration="1000" data-aos-delay="200">
    <div class="container">
        <h2>Our Team</h2>
        <hr>

    <div class="row justify-content-end">
        {teams.map((team) => <TeamList {...team} />)}
    </div>
    </div>
</section>
