---
import Layout from "../../layouts/Layout.astro"
import { Image } from 'astro:assets';

import Project1 from "../../images/sweden.webp"
---
<Layout title="Project | Sweden">
    <main class="sweden" id="sweden">
        <div class="container">
            <Image src={Project1} alt="white modern house" class="[ project-1 ] img-fluid"/>
            <div class="d-flex justify-content-between mt-2">
                <a href="../#projects" class=" border border-dark rounded-5 px-2"><svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 48 48"><g fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="4"><path d="M5.79889 24H41.7989"></path><path d="M17.7988 36L5.79883 24L17.7988 12"></path></g></svg> Back</a>
                <a href="././france" class=" border border-dark rounded-5 px-2">Next <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 48 48"><g fill="none" stroke="#000" stroke-linecap="round" stroke-linejoin="round" stroke-width="4"><path d="M41.9999 24H5.99994"></path><path d="M30 12L42 24L30 36"></path></g></svg></a>
            </div>
        </div>
    </main>
</Layout>

<style>
    .project-1{
        background-position: center;
        background-size: cover;
        background-attachment: fixed;
	  	background-repeat: no-repeat;
    }
</style>